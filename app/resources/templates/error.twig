{% extends "base.twig" %}

{% block body %}

<div class="row">
  <div class="col-md-8">
    <h3 class="mb-3">Error Notice</h3>
  </div>
</div>

<div class="row">
  <div class="col-md-8">
    <div class="card">
      <div class="card-header lead">
        An error occurred during processing the request.
      </div>
      <div class="card-body card-text">
        <div class="alert alert-danger" role="alert">
          {{ error.message }} {{ error.previous.message }}
        </div>      
      </div>
    </div>
  </div>
</div>

{% endblock %}